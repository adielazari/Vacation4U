<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Text?>

<GridPane alignment="center" hgap="10" prefHeight="151.0" prefWidth="317.0" vgap="10" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.UserSearchController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="295.0" minWidth="10.0" prefWidth="67.0" />
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="489.0" minWidth="10.0" prefWidth="240.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <Text fx:id="enter_username" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Username" />
        <Text fx:id="username" strokeType="OUTSIDE" strokeWidth="0.0" text="Username:" GridPane.rowIndex="1" />
        <Text fx:id="first_name" strokeType="OUTSIDE" strokeWidth="0.0" text="First Name:" GridPane.rowIndex="2" />
        <Text fx:id="last_name" strokeType="OUTSIDE" strokeWidth="0.0" text="Last Name:" GridPane.rowIndex="3" />
        <Text fx:id="birthday" strokeType="OUTSIDE" strokeWidth="0.0" text="Birth Day:" GridPane.rowIndex="4" />
        <Text fx:id="city" strokeType="OUTSIDE" strokeWidth="0.0" text="City:" GridPane.rowIndex="5" />
        <Text fx:id="error_message" fill="#da0e0e" strokeType="OUTSIDE" strokeWidth="0.0" text="Default Error" GridPane.rowIndex="6" />
        <TextField fx:id="enter_username_textfield" alignment="BASELINE_LEFT" prefHeight="25.0" prefWidth="150.0" GridPane.columnIndex="1" />
        <Text fx:id="username_display" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <Text fx:id="first_name_display" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="1" GridPane.rowIndex="2" />
        <Text fx:id="last_name_display" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="1" GridPane.rowIndex="3" />
        <Text fx:id="birthday_display" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="1" GridPane.rowIndex="4" />
        <Text fx:id="city_display" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="1" GridPane.rowIndex="5" />
        <Button fx:id="search_user_button" mnemonicParsing="false" onAction="#searchForUser" text="Search User" GridPane.columnIndex="1" GridPane.rowIndex="7" />
        <Button mnemonicParsing="false" onAction="#goBack" text="Go Back" GridPane.columnIndex="0" GridPane.rowIndex="7" />
    </children>
</GridPane>
